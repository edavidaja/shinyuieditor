{
  "name": "shiny-visual-editor",
  "version": "0.1.0",
  "homepage": "app",
  "private": true,
  "dependencies": {
    "@ladle/react": "^0.11.0",
    "@popperjs/core": "^2.11.2",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/jest": "^25.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "framer-motion": "^4.0.0",
    "immer": "^9.0.12",
    "just-clone": "^5.0.0",
    "just-debounce-it": "^3.0.0",
    "just-diff": "^4.1.2",
    "just-omit": "^1.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^3.0.0",
    "react-joyride": "^2.4.0",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "react-sortablejs": "^6.1.1",
    "sortablejs": "^1.15.0",
    "typescript": "^4.6.0-beta",
    "web-vitals": "^0.2.2",
    "yarn": "^1.22.18"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-grid": "REACT_APP_VERSION=GRIDAPP react-scripts start",
    "build": "react-scripts build",
    "build-netlify": "PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "ladle": "ladle serve",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint src --fix"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.tsx",
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/consistent-type-imports": "error",
          "import/order": [
            "error",
            {
              "groups": [
                "builtin",
                "external",
                "parent",
                "index",
                "internal"
              ],
              "pathGroups": [
                {
                  "pattern": "react",
                  "group": "external",
                  "position": "before"
                }
              ],
              "pathGroupsExcludedImportTypes": [
                "react"
              ],
              "newlines-between": "always",
              "alphabetize": {
                "order": "asc",
                "caseInsensitive": true
              }
            }
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8888",
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.13",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/sortablejs": "^1.10.7",
    "@typescript-eslint/parser": "^5.13.0",
    "cypress": "^8.5.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "2.5.1",
    "webpack": "5"
  }
}
